{
  "name": "@unique-nft/accounts",
  "version": "0.3.7",
  "license": "MIT",
  "dependencies": {
    "@polkadot/extension-dapp": "^0.47.1",
    "@polkadot/types": "^10.13.1",
    "@polkadot/ui-keyring": "^3.6.6",
    "@polkadot/util": "^12.6.2",
    "@unique-nft/utils": "^0.3.12",
    "ethers": "^5.7.1",
    "reflect-metadata": "^0.1.13"
  },
  "description": "The package is required to connect different accounts and create signatures for them.",
  "keywords": [
    "blockchain",
    "parachain",
    "polkadot",
    "unique network",
    "nft"
  ],
  "bugs": {
    "url": "https://github.com/UniqueNetwork/unique-sdk/issues"
  },
  "author": {
    "name": "Unique Network SDK team"
  },
  "homepage": "https://unique.network/",
  "repository": {
    "type": "git",
    "url": "https://github.com/UniqueNetwork/unique-sdk.git"
  },
  "type": "module",
  "types": "index.d.ts",
  "main": "index.cjs",
  "module": "index.js",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./index.cjs",
      "default": "./index.js"
    },
    "./types": {
      "types": "./types/index.d.ts",
      "require": "./types/index.cjs",
      "default": "./types/index.js"
    },
    "./keyring": {
      "types": "./keyring/index.d.ts",
      "require": "./keyring/index.cjs",
      "default": "./keyring/index.js"
    },
    "./keyring-local": {
      "types": "./keyring-local/index.d.ts",
      "require": "./keyring-local/index.cjs",
      "default": "./keyring-local/index.js"
    },
    "./polkadot": {
      "types": "./polkadot/index.d.ts",
      "require": "./polkadot/index.cjs",
      "default": "./polkadot/index.js"
    },
    "./metamask": {
      "types": "./metamask/index.d.ts",
      "require": "./metamask/index.cjs",
      "default": "./metamask/index.js"
    }
  }
}